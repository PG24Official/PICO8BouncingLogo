pico-8 cartridge // http://www.pico-8.com
version 34
__lua__
--pico-8 screensaver
--by pg24

function _init()
 x=rnd(128)
 y=rnd(128)
 
 xrate=1
 yrate=0.5
 
 sprite=1
 
 hits=0
end

function _update()
 x=x+xrate
 y=y+yrate
 
 if x>121 then
  xrate=-1
  hits+=1
 end
 if y>120 then
  yrate=-0.5
  hits+=1
 end
 
 if x<1 then
  xrate=1
  hits+=1
 end
 if y<1 then
  yrate=0.5
  hits+=1
 end
 
 if btnp(⬆️)
 and sprite !=8 then
  sprite+=1
 end
 
 if btnp(⬇️)
 and sprite !=1 then
  sprite-=1
 end
 
end
function _draw()
 cls()
 spr(sprite,x,y)
 
 print("hits: ",0,0)
 print(hits,0,8)
end
